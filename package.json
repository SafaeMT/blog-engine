{
  "name": "blog-engine",
  "version": "1.0.0",
  "description": "> A build to learn project",
  "scripts": {
    "start:client": "cd client && npm start",
    "build:client": "cd client && npm run build",
    "test:client": "cd client && npm test",
    "lint:client": "cd client && npm run lint",
    "format": "prettier 'client/{public,src}/**/*.{html,css,js}' 'server.js' '.circleci/config.yml'",
    "prepare": "husky install",
    "start:server": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SafaeMT/blog-engine.git"
  },
  "keywords": [],
  "author": "SafaeMT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SafaeMT/blog-engine/issues"
  },
  "homepage": "https://github.com/SafaeMT/blog-engine#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "react-test-renderer": "^17.0.2"
  },
  "lint-staged": {
    ".circleci/config.yml": "prettier --write",
    "compose.yaml": "prettier --write",
    "client/{public,src}/**/*.{html,css,js}": "prettier --write",
    "client/src/*.js": "npm run lint:client",
    "server.js": "prettier --write"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.9",
    "react-router-dom": "^5.2.0"
  }
}
