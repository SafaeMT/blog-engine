{
  "name": "blog-engine",
  "version": "1.0.0",
  "description": "> A build to learn project",
  "scripts": {
    "build:client": "parcel build client/public/index.html",
    "format": "prettier --write \"client/{public,src}/**/*.{html,css,js}\" \"server.js\"",
    "lint": "eslint \"client/src/**/*.js\" \"server.js\"",
    "prepare": "husky install",
    "start:client": "parcel serve client/public/index.html",
    "start:server": "nodemon server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SafaeMT/blog-engine.git"
  },
  "keywords": [],
  "author": "SafaeMT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SafaeMT/blog-engine/issues"
  },
  "homepage": "https://github.com/SafaeMT/blog-engine#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "parcel": "*",
    "prettier": "^2.3.0"
  },
  "lint-staged": {
    "client/{public,src}/**/*.{html,css,js}": "prettier --write",
    "client/src/**/*.js": "eslint",
    "server.js": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
}
