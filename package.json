  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SafaeMT/blog-engine/issues"
  },
  "homepage": "https://github.com/SafaeMT/blog-engine#readme",
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7"
  },
  "lint-staged": {
    "client/src/*.js": "npm run lint:client",
    "client/**/*": "npm run format:client"
  },
  "dependencies": {
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
