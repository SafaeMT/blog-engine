{
  "name": "blog-engine",
  "version": "1.0.0",
  "description": "> A build to learn project",
  "scripts": {
    "build:client": "parcel build client/public/index.html",
    "cypress:open": "cypress open",
    "format": "prettier 'client/{public,src}/**/*.{html,css,js}' 'cypress/**/*.{js,json}' 'server.js'",
    "lint": "eslint 'client/src/**/*.js' 'cypress/**/*.js' 'server.js'",
    "prepare": "husky install",
    "start:client": "parcel serve client/public/index.html",
    "start:server": "nodemon server.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SafaeMT/blog-engine.git"
  },
  "keywords": [],
  "author": "SafaeMT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SafaeMT/blog-engine/issues"
  },
  "homepage": "https://github.com/SafaeMT/blog-engine#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/react": "^11.2.7",
    "babel-jest": "^27.0.2",
    "cypress": "^7.5.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "parcel": "2.0.0-beta.3.1",
    "prettier": "^2.3.0",
    "react-test-renderer": "^17.0.2"
  },
  "lint-staged": {
    "client/{public,src}/**/*.{html,css,js}": "prettier --write",
    "client/src/**/*.js": "eslint",
    "cypress/**/*.js": [
      "prettier --write",
      "eslint"
    ],
    "server.js": [
      "prettier --write",
      "eslint"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "dotenv": "^7.0.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.9",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  }
}
